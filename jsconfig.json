{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@app/*": [
        "src/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@shared/*": [
        "src/shared/*"
      ],
      "@store/*": [
        "src/store/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@views/*": [
        "src/view/*"
      ]
    }
  },
  "include": [
    "src/**/*"
  ]
}
